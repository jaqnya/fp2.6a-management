#REGION 1
PROCEDURE NextNumber

PRIVATE pnSelect, pcOrder, pnRecNo, NextCode
pnSelect = SELECT()
pcOrder  = ORDER()
pnRecNo  = IIF(EOF(), 0, RECNO())

SELECT familias
SET ORDER TO TAG indice1 OF familias.cdx
GO BOTTOM

NextCode = familias.codigo + 1

SELECT (pnSelect)
SET ORDER TO TAG (pcOrder)
IF pnRecNo <> 0
   GOTO RECORD pnRecNo
ENDIF

RETURN (NextCode)

PROCEDURE _ShowSave
IF m.nombre <> familias.nombre .OR. ;
   m.p1 <> familias.p1 .OR. ;
   m.p2 <> familias.p2 .OR. ;
   m.p3 <> familias.p3 .OR. ;
   m.p4 <> familias.p4 .OR. ;
   m.p5 <> familias.p5 .OR. ;
   IIF(m.vigente = "S", .T., .F.) <> familias.vigente
   SHOW GET pnChoice, 1 ENABLE
ELSE
   SHOW GET pnChoice, 1 DISABLE
ENDIF

