      SET ORDER TO TAG indice1
   ELSE
      IF m.ord_rubr = "S"
         SELECT tmpmaesprod
         SET ORDER TO TAG indice14
      ENDIF
   ENDIF
ELSE
   IF m.ordenado = "N"
      IF m.ord_rubr = "N"
         SELECT tmpmaesprod
         SET ORDER TO TAG indice2
      ELSE
         IF m.ord_rubr = "S"
