      DEFINE WINDOW brwruta ;
         FROM 01,00 ;
         TO   23,60 ;
         TITLE "< RUTAS >" ;
         SYSTEM ;
         CLOSE ;
         FLOAT ;
         GROW ;
         MDI ;
         NOMINIMIZE ;
         SHADOW ;
         ZOOM ;
         COLOR SCHEME 15
