      USE ruta IN 0 SHARED
   ENDIF

   ON KEY LABEL "ENTER" KEYBOARD "{CTRL+W}"

   SELECT ruta
   BROWSE WINDOW brwruta FIELDS ;
      id_ruta :R:06:H = "C¢digo",;
      nombre  :R:50:H = "Nombre";
      NOAPPEND NODELETE NOMODIFY

   ON KEY LABEL "ENTER"

   IF LASTKEY() = 27 THEN
      RETURN 0
   ENDIF

   m.ruta = id_ruta
ENDIF

